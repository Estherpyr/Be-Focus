{
  "name": "Be Focus",
  "manifest_version": 2,
  "version": "0.1",
  "content_scripts":[
    {
      "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "glasses.png",
    "default_title": "Be Focus"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
